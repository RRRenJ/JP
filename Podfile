# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
install! 'cocoapods', :warn_for_unused_master_specs_repo => false

target 'JPSDK' do
 
  use_frameworks!
  


  pod 'GPUImage',:git => 'https://github.com/RRRenJ/JPGPUImage.git'
  pod 'SDCycleScrollView'
  pod 'SDAutoLayout'
  pod 'MBProgressHUD'
  pod 'Masonry'
  pod 'FMDB'
  pod 'MJRefresh'

end
